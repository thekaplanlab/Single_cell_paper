data3$Count_p<-(data3$Count*100)/(data3$Count + data1$Count + data2$Count + data4$Count)
data4$Count_p<-(data4$Count*100)/(data3$Count + data1$Count + data2$Count + data4$Count)
data_last<-rbind(data1, data2, data3, data4)
